<!%= debug @issues %>

<h3>Search Results</h3>
<% @issues.each do |issue| %>
	<div class='row'>
		<div class='col-sm-6'>
			<h5><%= issue.name %></h5>
			
			
		</div>
		<div class='col-sm-6'>
			<%= issue.created_at %>
		</div>
	</div>
	<div class='row'>
		<div class='col-sm-12'>
			<i><%= issue.description %></i>
		</div>
	</div>
<% end %>

<%= will_paginate @issues, renderer: BootstrapPagination::Rails %>