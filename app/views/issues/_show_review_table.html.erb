<br>
<% if list.count < 1 %>
	<div class='alert alert-info' role='alert'>
		No <%= name %> are added in yet. <%= link %>
	</div>
<% else %>
	<table class='table'>
		<tr>
			<th>Description</th>
			<th>Images</th>
		</tr>
		<% list.each do |workaround| %>
			<tr>
				<td><%= workaround.description %></td>
				<td><%= display_images(workaround.images) %></td>
			</tr>
		<% end %>
	</table>
<% end %>